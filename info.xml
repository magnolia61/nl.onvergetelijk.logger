<?xml version="1.0"?>
<extension key="nl.onvergetelijk.logger" type="module">
  <file>logger</file>
  <name>Onvergetelijke Zomerkampen - Logger</name>
  <description>Deze extensie checkt diverse logs en foutmeldingen</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Richard van Oosterhout</author>
    <email>webteam@onvergetelijk.nl</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2026-03-01</releaseDate>
  <version>2.0.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>6.11</ver>
  </compatibility>
  <comments>This extension is used by Stichting Onvergetelijke Zomerkampen</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Logger</namespace>
    <format>23.02.1</format>
    <angularModule>crmLogger</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
